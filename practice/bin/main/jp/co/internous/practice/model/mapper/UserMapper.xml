<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.co.internous.practice.model.mapper.UserMapper">

  <select id="findByUserNameAndPassword" resultType="jp.co.internous.practice.model.domain.User">
    SELECT * FROM user WHERE user_name = #{userName} AND password = #{password}
